{
  "name": "transcrobes",
  "version": "0.11.0",
  "private": true,
  "scripts": {
    "validate": "tsc --noEmit",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack --config webpack.config.site.ts --mode=development",
    "build:prod": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack --config webpack.config.site.ts --mode=production",
    "buildext:dev": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack --config webpack.config.ext.ts --mode=development",
    "buildext:prod": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack --config webpack.config.ext.ts --mode=production",
    "buildext:release": "rm -f ./extdist/brocrobes-$(git describe --tags).zip && npm run buildext:prod && cd extbuild && mkdir -p ../extdist && zip -r ../extdist/brocrobes-$(git describe --tags).zip *"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "browserslist": [
    "last 3 chrome version",
    "last 2 edge version",
    "last 2 and_chr version"
