{
  "name": "transcrobes",
  "version": "0.28.2",
  "private": true,
  "type": "module",
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "browserslist": [
    "last 3 chrome version",
    "last 2 edge version",
    "last 2 and_chr version"
  ],
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "validate": "tsc --noEmit",
    "run:dev": "tsc --noEmit && rimraf dev-dist && cross-env DEBUG=vite-plugin-pwa:* VITE_GIT_VERSION=$(git describe --tags) SW_DEV=true SW=true vite --force --config vite.config.ts --mode=development",
    "build:prod": "tsc --noEmit && rm -rf dist/* && cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true SW=true NODE_OPTIONS='--max-old-space-size=8192' vite build --config vite.config.ts --mode=production",
    "run:prod": "npm run build:prod && npm run serve",
    "serve": "serve -p 5000 dist",
    "buildext:dev": "tsc --noEmit && npm run buildext:nf && cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true SW=true NODE_OPTIONS='--max-old-space-size=8192' vite --force --config vite.ext.config.ts --mode=development",
    "buildext:nf": "cross-env BASE_URL=/ SOURCE_MAP=true vite build --config vite.nf.config.ts --mode=production",
    "buildext:devbuild": "tsc --noEmit && rimraf extbuild && npm run buildext:nf && cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true SW=true NODE_OPTIONS='--max-old-space-size=8192' vite build --config vite.ext.config.ts --mode=development",
    "buildext:prod": "tsc --noEmit && npm run buildext:nf && rimraf extbuild && cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true SW=true NODE_OPTIONS='--max-old-space-size=8192' vite build --config vite.ext.config.ts --mode=production",
    "buildext:release": "rm -f ./extdist/brocrobes-$(git describe --tags).zip && npm run buildext:prod && cd extbuild && mkdir -p ../extdist && zip -r ../extdist/brocrobes-$(git describe --tags).zip assets manifest.json nf.iife.js public service-worker-loader.js src static"
  },
